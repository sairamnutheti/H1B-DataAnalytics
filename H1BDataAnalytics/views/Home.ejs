		<script>
function createChart() {
            $("#chart2").kendoChart({
                dataSource: {
                    transport: {
                        read: {
                            url: "/data",
                            dataType: "json"
                        }
                    },
                    sort: {
                        field: "year",
                        dir: "asc"
                    }
                },
                title: {
                    text: "Number of H1B's sponsered yearwise "
                },
                legend: {
                    position: "top"
                },
                seriesDefaults: {
                    type: "column"
                },
                series: [{
                    field: "Amazon",
                    name: "Amazon"
                }, {
                    field: "Google",
                    name: "Google"
                }, {
                    field: "IBM",
                    name: "IBM"
                } , {
                    field: "Apple",
                    name: "Apple"
                }
                ],
                categoryAxis: {
                    field: "year",
                    labels: {
                        rotation: -90
                    },
                    crosshair: {
                        visible: true
                    }
                },
                valueAxis: {
                    labels: {
                        format: "N0"
                    },
                    majorUnit: 1000
                },
                tooltip: {
                    visible: true,
                    shared: true,
                    format: "N0"
                }
            });




            $("#chart1").kendoChart({
                dataSource: {
                    transport: {
                        read: {
                            url: "/data",
                            dataType: "json"
                        }
                    },
                    sort: {
                        field: "year",
                        dir: "asc"
                    }
                },
                title: {
                    text: "Number of H1B's sponsered yearwise "
                },
                legend: {
                    position: "top"
                },
                seriesDefaults: {
                    type: "line"
                },
                series: [{
                    field: "Amazon",
                    name: "Amazon"
                }, {
                    field: "Google",
                    name: "Google"
                }, {
                    field: "IBM",
                    name: "IBM"
                } , {
                    field: "Apple",
                    name: "Apple"
                }
                ],
                categoryAxis: {
                    field: "year",
                    labels: {
                        rotation: -90
                    },
                    crosshair: {
                        visible: true
                    }
                },
                valueAxis: {
                    labels: {
                        format: "N0"
                    },
                    majorUnit: 1000
                },
                tooltip: {
                    visible: true,
                    shared: true,
                    format: "N0"
                }
            });
        }

        $(document).ready(createChart);
    
    </script>
